<Window x:Class="VideoClub.Main_President"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="Президент" Height="450" Width="800" Closed="Window_Closed">

    <DockPanel>
        <ToolBar DockPanel.Dock="Top" FontSize="20">
            <Button Name="top_button" Content="Хит-парад" Click="top_button_Click" />
            <Button Name="anti_button" Content="Антихит-парад" Click="anti_button_Click" />
            <Button Name="replace_button" Content="Требуется замена" Click="replace_button_Click" />
            <Button Name="librarians_button" Content="Библиотекари" Click="librarians_button_Click" />
            <Button Content="Справка" />
        </ToolBar>

        <Frame Name="frame" NavigationUIVisibility="Hidden" />
    </DockPanel>
</Window>